    "require-unicode-regexp": 0,
    "require-yield": 0,
    "sort-imports": 0,
    "sort-keys": 0,
    "sort-vars": 0,
    "spaced-comment": 0,
    "strict": 0,
    "symbol-description": 0,
    "vars-on-top": 0,
    "yoda": 0,
    "array-bracket-newline": 0,
