    "no-restricted-globals": 0,
    "no-restricted-imports": 0,
    "no-restricted-properties": 0,
    "no-restricted-syntax": 0,
    "no-return-assign": 0,
    "no-return-await": 0,
    "no-script-url": 0,
    "no-sequences": 0,
    "no-shadow": 0,
    "no-shadow-restricted-names": 0,
    "no-ternary": 0,
    "no-throw-literal": 0,
    "no-undef-init": 0,
    "no-undefined": 0,
    "no-underscore-dangle": 0,
