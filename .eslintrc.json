    "use-isnan": 0,
    "valid-typeof": 0,
    "accessor-pairs": 0,
    "arrow-body-style": 0,
    "block-scoped-var": 0,
    "camelcase": 0,
    "capitalized-comments": 0,
    "class-methods-use-this": 0,
    "complexity": 0,
    "consistent-return": 0,
    "consistent-this": 0,
    "curly": 0,
    "default-case": 0,
